{
  "name": "juice_shop_playwright",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "scripts": {
    "test": "playwright test",
    "test:e2e": "playwright test --config=e2e.config.ts --project=chrome",
    "db:setup": "npx ts-node scripts/setup-database.ts",
    "db:example": "npx ts-node scripts/example-mysql-usage.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "load-test": "k6 run load-tests/main.ts",
    "load-test:smoke": "k6 run -e SCENARIO=smoke load-tests/main.ts",
    "load-test:load": "k6 run -e SCENARIO=load load-tests/main.ts",
    "load-test:stress": "k6 run -e SCENARIO=stress load-tests/main.ts",
    "load-test:spike": "k6 run -e SCENARIO=spike load-tests/main.ts",
    "load-test:results": "k6 run -e SAVE_RESULTS=true load-tests/main.ts",
    "load-test:cloud": "k6 run --out cloud load-tests/main.ts",
    "load-test:cloud:smoke": "k6 run --out cloud -e SCENARIO=smoke load-tests/main.ts",
    "load-test:cloud:load": "k6 run --out cloud -e SCENARIO=load load-tests/main.ts",
    "load-test:cloud:stress": "k6 run --out cloud -e SCENARIO=stress load-tests/main.ts",
    "load-test:cloud:spike": "k6 run --out cloud -e SCENARIO=spike load-tests/main.ts",
    "mcp:run": "npx tsx mcp-server.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@playwright/test": "^1.57.0",
    "@types/dotenv": "^6.1.1",
    "@types/k6": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "allure-commandline": "^2.36.0",
    "allure-playwright": "^3.4.4",
    "dotenv": "^16.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-playwright": "^2.5.0",
    "openai": "^6.16.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.3",
    "date-fns": "^3.6.0",
    "mysql2": "^3.16.1"
  }
}
